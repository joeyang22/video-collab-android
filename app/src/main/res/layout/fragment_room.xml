<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="me.joeyang.musiccollab.Fragments.RoomFragment"
    android:id="@+id/roomFragment">
    <TextView
        android:id="@+id/roomNameTextView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Room: ABCD"
        android:textAlignment="center"
        android:textStyle="bold"
        android:layout_marginTop="16dp"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"/>
    <FrameLayout
        android:id="@+id/youtubeSupportFragment"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:visibility="visible"
        android:layout_below="@id/roomNameTextView"
        android:layout_margin="16dp"/>

    <Button
        android:layout_below="@id/youtubeSupportFragment"
        android:id="@+id/addVideoButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/add_video"
        android:onClick="addVideo"/>
    <Button
        android:layout_below="@id/addVideoButton"
        android:id="@+id/voteVideoButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Voting for video"
        android:onClick="voteVideo"/>
    <EditText
        android:id="@+id/videoIdEditText"
        android:layout_below="@id/voteVideoButton"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"/>


</RelativeLayout>
